<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.customer.InvestIntention"
        table="invest_intention"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="intentId"
            column="intent_id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="addrDemand"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="addr_demand"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="dangerDemand"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="danger_demand"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="intentDate"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="intent_date"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="intentMoney"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="intent_money"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="issingleDemand"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="issingle_demand"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="otherInvest"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="other_invest"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="status"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="investPersonId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="invest_person_id"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="investPersonName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="invest_person_name"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="findPerson"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="invest_find_person"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="endDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="invest_end_date"
            length="10"
            not-null="false"
            unique="false"
        />
        <many-to-one name="investPerson" class="com.zhiwei.credit.model.customer.InvestPerson" column="invest_person" lazy="false"></many-to-one>
     </class>
</hibernate-mapping>
