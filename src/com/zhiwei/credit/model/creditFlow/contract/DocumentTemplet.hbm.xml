<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zhiwei.credit.model.creditFlow.contract.DocumentTemplet" table="cs_document_templet" >
		 <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="text" type="java.lang.String">
            <column name="text" length="224"/>
        </property>
        <property name="onlymark" type="java.lang.String">
            <column name="onlymark" length="224"/>
        </property>
        <property name="parentid" type="java.lang.Integer">
            <column name="parentid" length="24"/>
        </property>
        <property name="leaf" type="java.lang.Integer">
            <column name="leaf" length="1"/>
        </property>
        <property name="isexist" type="java.lang.Boolean">
            <column name="isexist" length="1" not-null="true"/>
        </property>
        <property name="templettype" type="java.lang.Integer">
            <column name="templettype" length="24"/>
        </property>
        <property name="fileid" type="java.lang.Integer">
            <column name="fileid" length="24" not-null="true"/>
        </property>
        <property name="remarks" type="java.lang.String">
            <column name="remarks" length="224"/>
        </property>
        <property name="textoo" type="java.lang.String">
            <column name="textoo" length="240"/>
        </property>
         <property name="businessType" type="java.lang.String">
            <column name="businesstype" length="30"/>
        </property>
        <property name="handleFun" type="java.lang.String">
            <column name="handleFun" length="440"/>
        </property>
        <property name="orderNum" type="java.lang.Integer">
            <column name="ordernum" length="11"/>
        </property>
        <property name="isChild" type="java.lang.Integer">
            <column name="isChild" length="11"/>
        </property>
	</class>
</hibernate-mapping>