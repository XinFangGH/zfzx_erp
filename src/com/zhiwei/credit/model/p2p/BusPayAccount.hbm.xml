<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.p2p.BusPayAccount"
        table="bus_pay_account"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="accountType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="accountType"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="trueName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="trueName"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="account"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="account"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="accountName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="accountName"
            length="100"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
