<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.finance.ProfitRateMaintenance"
        table="profit_rate_maintenance"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="rateId"
            column="rateId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="adjustDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="adjustDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="sixMonthLow"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="sixMonthLow"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="oneYear"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="oneYear"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="threeYear"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="threeYear"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="fiveYear"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="fiveYear"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="fiveYearUp"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="fiveYearUp"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createTime"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="creator"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="creator"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="updateTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="updateTime"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="updator"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="updator"
            length="100"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
