<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.credit.proj.entity.CsProcreditMortgageReceivables"
        table="cs_procredit_mortgage_receivables"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="receivablesId"
            column="receivablesId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="enterpriseId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="enterpriseId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="enterpriseName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="enterpriseName"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="accountsPayCompany"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="accountsPayCompany"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="receivableMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="receivableMoney"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="contractNumber"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="contractNumber"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="intentDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="intentDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="billNumber"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="billNumber"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="billDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="billDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="mortgageId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="mortgageId"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="objectType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="objectType"
            length="100"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
