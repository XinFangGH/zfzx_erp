<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping> 
  <class name="com.zhiwei.credit.model.system.Dictionary" table="dictionary" select-before-update="false" dynamic-update="false" dynamic-insert="false" optimistic-lock="version"> 
    <id name="dicId" column="dicId" type="java.lang.Long" length="20"> 
      <generator class="native"></generator> 
    </id>  
    <property name="itemName" type="java.lang.String" update="true" insert="true" access="property" column="itemName" length="64" not-null="true" unique="false"/>  
    <property name="itemValue" type="java.lang.String" update="true" insert="true" access="property" column="itemValue" length="128" not-null="true" unique="false"/>  
    <property name="sn" type="java.lang.Short" update="true" insert="true" access="property" column="sn" length="11" not-null="false" unique="false"/>  
    <property name="dicKey" type="java.lang.String" update="true" insert="true" access="property" column="dicKey" length="50" not-null="true" unique="true"/> 
    <property name="descp" type="java.lang.String" update="true" insert="true" access="property" column="descp" length="65535" not-null="false" unique="false"/>  
    <property name="status" type="java.lang.String" update="true" insert="true" access="property" column="status" length="30" not-null="true" unique="true"/> 
    <many-to-one name="globalType" class="com.zhiwei.credit.model.system.GlobalType" not-null="false" fetch="select"> 
      <column name="proTypeId"/> 
    </many-to-one> 
  </class> 
</hibernate-mapping>
