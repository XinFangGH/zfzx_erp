<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.p2p.P2pFriendlink"
        table="p2p_friendlink"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="modifyDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="modifyDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="logo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="logo"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="name"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="orderList"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="orderList"
            length="10"
            not-null="true"
            unique="false"
        />
        <property
            name="url"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="url"
            length="255"
            not-null="true"
            unique="false"
        />
        <property 
        	name="linkType"
			type="java.lang.Short"
			update="true"
			insert="true"
			access="property"
			column="linkType"
			length="5"
			not-null="false"
			unique="false"
        />
         <property
            name="webKey"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="webKey"
            length="19"
            not-null="false"
            unique="false"
        />
           <property
            name="isShow"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isShow"
            length="5"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
