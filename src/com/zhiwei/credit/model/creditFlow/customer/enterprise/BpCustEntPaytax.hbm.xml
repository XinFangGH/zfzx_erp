<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.customer.enterprise.BpCustEntPaytax"
        table="bp_cust_ent_paytax"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="paytaxsId"
            column="paytaxsId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
         <property
            name="year"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="year"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="increaseTaxe"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="increaseTaxe"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="salesTax"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="salesTax"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="incomeTax"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="incomeTax"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="customersTax"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="customersTax"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="additionTax"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="additionTax"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="ohterTax"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="ohterTax"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
  <property
            name="sumTax"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="sumTax"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
         <many-to-one name="enterprise" class="com.zhiwei.credit.model.creditFlow.customer.enterprise.Enterprise"   > 
        <column name="enterpriseId"/> 
        </many-to-one> 
     </class>
</hibernate-mapping>
