<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.archives.OurArchivesMaterials"
        table="our_archives_materials"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="materialsId"
            column="materialsId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="materialsName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="materialsName"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="operationTypeKey"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="operationTypeKey"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="isPublic"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isPublic"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="businessTypeKey"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="businessTypeKey"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="businessTypeName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="businessTypeName"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="businessTypeGlobalId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="businessTypeGlobalId"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="operationTypeGlobalId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="operationTypeGlobalId"
            length="20"
            not-null="false"
            unique="false"
        />
        
        <property
            name="productId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="productId"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="settingId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="settingId"
            length="20"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
