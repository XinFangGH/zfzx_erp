<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="businessandlive" extends="struts-default">
		<!-- 添加信息 -->
		<action name="addBusinessAndLive" class="businessandliveAction" method="addBusinessandlive">
			<result>${successResultValue}</result>
		</action>
		
		<!-- 查看信息从视图查询 -->
		<action name="seeBusinessAndLive" class="businessandliveAction" method="seeBusinessandlive"></action>
		
		<!-- 查看信息从商住表查询显示在更新页面- -->
		<action name="seeBusinessAndLiveForUpdate" class="businessandliveAction" method="seeBusinessAndLiveForUpdate"></action>
		
		<!-- 更新信息 -->
		<action name="updateBusinessAndLive" class="businessandliveAction" method="updateBusinessandlive" ></action>
		
	</package>
</struts>  