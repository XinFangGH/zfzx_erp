<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.pawn.pawnItems.PawnInspectionRecord"
        table="pawn_inspection_record"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="inspectionId"
            column="inspectionId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="operateType"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="operateType"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="inspectorId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="inspectorId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="inspectorName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="inspectorName"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="matterDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="matterDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="remarks"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remarks"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="creatorName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="creatorName"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createTime"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="pawnItemId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="pawnItemId"
            length="19"
            not-null="false"
            unique="false"
        />
         <property
            name="projectId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="projectId"
            length="19"
            not-null="false"
            unique="false"
        />
         <property
            name="businessType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="businessType"
            length="100"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
