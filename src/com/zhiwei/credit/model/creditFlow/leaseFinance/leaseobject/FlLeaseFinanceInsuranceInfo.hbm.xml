<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.leaseFinance.leaseobject.FlLeaseFinanceInsuranceInfo"
        table="fl_lease_finance_insurance_info"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="insuranceId"
            column="insuranceId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="insuranceName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="insuranceName"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="insuranceCompanyName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="insuranceCompanyName"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="insuranceCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="insuranceCode"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="insuranceMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="insuranceMoney"
            precision="12"
            scale="4"
            not-null="false"
            unique="false"
        />
        <property
            name="startDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="startDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="endDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="endDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="insurancePerson"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="insurancePerson"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="insuranceComment"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="insuranceComment"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="leaseObjectId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="leaseObjectId"
            length="19"
            not-null="true"
            unique="false"
        />
     </class>
</hibernate-mapping>
