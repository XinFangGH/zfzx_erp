<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="officebuilding" extends="struts-default">
		<!-- 添加信息 -->
		<action name="addOfficebuilding" class="officebuildingAction" method="addOfficebuilding">
			<result>${successResultValue}</result>
		</action>
		
		<!-- 查看信息从视图 -->
		<action name="seeOfficebuilding" class="officebuildingAction" method="seeOfficebuilding"></action>
		
		<!-- 查看信息从商品写字楼表查询数据显示在更新页面 -->
		<action name="seeOfficebuildingForUpdate" class="officebuildingAction" method="seeOfficebuildingForUpdate"></action>
		
		<!-- 更新信息 -->
		<action name="updateOfficebuilding" class="officebuildingAction" method="updateOfficebuilding" ></action>
		
	</package>
</struts>