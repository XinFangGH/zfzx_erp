<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping> 
  <class name="com.zhiwei.credit.model.archive.ArchDispatch" table="arch_dispatch" select-before-update="false" dynamic-update="false" dynamic-insert="false" optimistic-lock="version"> 
    <id name="dispatchId" column="dispatchId" type="java.lang.Long" length="19"> 
      <generator class="native"></generator> 
    </id>  
    <property name="dispatchTime" type="java.util.Date" update="true" insert="true" access="property" column="dispatchTime" length="19" not-null="true" unique="false"/>  
    <property name="userId" type="java.lang.Long" update="true" insert="true" access="property" column="userId" length="19" not-null="false" unique="false"/>  
    <property name="fullname" type="java.lang.String" update="true" insert="true" access="property" column="fullname" length="128" not-null="false" unique="false"/>  
    <property name="isRead" type="java.lang.Short" update="true" insert="true" access="property" column="isRead" length="5" not-null="false" unique="false"/>  
    <property name="subject" type="java.lang.String" update="true" insert="true" access="property" column="subject" length="256" not-null="false" unique="false"/>  
    <property name="readFeedback" type="java.lang.String" update="true" insert="true" access="property" column="readFeedback" length="1024" not-null="false" unique="false"/>  
    <property name="archUserType" type="java.lang.Short" update="true" insert="true" access="property" column="archUserType" length="5" not-null="false" unique="false"/>  
    <property name="disRoleId" type="java.lang.Long" update="true" insert="true" access="property" column="disRoleId" length="19" not-null="false" unique="false"/>  
    <property name="disRoleName" type="java.lang.String" update="true" insert="true" access="property" column="disRoleName" length="128" not-null="false" unique="false"/>  
    <!-- imported bi-directional many-to-one association to Archives -->  
    <many-to-one name="archives" class="com.zhiwei.credit.model.archive.Archives" not-null="false" fetch="select"> 
      <column name="archivesId"/> 
    </many-to-one> 
  </class> 
</hibernate-mapping>
