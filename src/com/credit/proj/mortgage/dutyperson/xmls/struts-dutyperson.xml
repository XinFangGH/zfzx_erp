<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="dutyperson" extends="struts-default">
		<!-- 添加信息 -->
		<action name="addDutyperson" class="dutypersonAction" method="addDutyperson">
			<result>${successResultValue}</result>
		</action>
		
		<!-- 查看信息从视图查询 -->
		<action name="seeDutyperson" class="dutypersonAction" method="seeDutyperson"></action>
		
		<!-- 查看信息从责任个人表查询数据显示在更新页面 -->
		<action name="seeDutypersonForUpdate" class="dutypersonAction" method="seeDutypersonForUpdate"></action>
		
		<!-- 更新信息 -->
		<action name="updateDutyperson" class="dutypersonAction" method="updateDutyperson"></action>
		
	</package>
</struts>  