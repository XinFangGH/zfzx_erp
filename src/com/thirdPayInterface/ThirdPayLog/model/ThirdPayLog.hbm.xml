<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.thirdPayInterface.ThirdPayLog.model.ThirdPayLog"
        table="third_pay_log"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="logId"
            column="logId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
        
         <property
            name="uniqueId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="uniqueId"
            length="50"
            not-null="false"
            unique="false"
        />
         <property
            name="userId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="userId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="thirdPayConfig"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="thirdPayConfig"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="interfaceType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="interfaceType"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="interfaceName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="interfaceName"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="requestTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="requestTime"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="requestNum"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="requestNum"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="status"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="recordNumber"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="recordNumber"
            length="255"
            not-null="false"
            unique="true"
        />
        <property
            name="codeMsg"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="codeMsg"
            length="255"
            not-null="false"
            unique="false"
        />
       
     </class>
</hibernate-mapping>
