<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.leaseFinance.supplior.FlObjectSupplior"
        table="fl_object_supplior"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="legalPersonName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="legalPersonName"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="companyPhoneNum"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="companyPhoneNum"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="Name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="Name"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="connectorName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="connectorName"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="connectorPhoneNum"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="connectorPhoneNum"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="connectorPosition"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="connectorPosition"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="companyFax"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="companyFax"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="companyAddress"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="companyAddress"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="companyComment"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="companyComment"
            length="255"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
