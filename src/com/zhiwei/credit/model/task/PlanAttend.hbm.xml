<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping> 
  <class name="com.zhiwei.credit.model.task.PlanAttend" table="plan_attend" select-before-update="false" dynamic-update="false" dynamic-insert="false" optimistic-lock="version"> 
    <id name="attendId" column="attendId" type="java.lang.Long" length="19"> 
      <generator class="native"></generator> 
    </id>  
    <property name="isDep" type="java.lang.Short" update="true" insert="true" access="property" column="isDep" length="5" not-null="true" unique="false"/>  
    <property name="isPrimary" type="java.lang.Short" update="true" insert="true" access="property" column="isPrimary" length="5" not-null="false" unique="false"/>  
    <!-- imported bi-directional many-to-one association to WorkPlan -->  
    <many-to-one name="workPlan" class="com.zhiwei.credit.model.task.WorkPlan" not-null="false" fetch="select"> 
      <column name="planId"/> 
    </many-to-one>  
    <!-- imported bi-directional many-to-one association to Department -->  
    <many-to-one name="department" class="com.zhiwei.credit.model.system.Department" not-null="false" fetch="select"> 
      <column name="depId"/> 
    </many-to-one>  
    <!-- imported bi-directional many-to-one association to AppUser -->  
    <many-to-one name="appUser" class="com.zhiwei.credit.model.system.AppUser" not-null="false" fetch="select"> 
      <column name="userId"/> 
    </many-to-one> 
  </class> 
</hibernate-mapping>
