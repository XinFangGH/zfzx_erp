<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.common.BpPageSetting"
        table="bp_page_setting"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="pageSetId"
            column="pageSetId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="pageName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="pageName"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="parentId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="parentId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="pageContent"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="pageContent"
            length="65535"
            not-null="false"
            unique="false"
        />
        <property
            name="pageKey"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="pageKey"
            length="100"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
