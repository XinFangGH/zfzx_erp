<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.finance.FundsToPromote"
        table="funds_to_promote"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="fundsToPromoteId"
            column="fundsToPromoteId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="promoteDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="promoteDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="promotePersonId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="promotePersonId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="promotePersonName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="promotePersonName"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="investPersonId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="investPersonId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="investPersonName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="investPersonName"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="title"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="title"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="promoteContext"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="promoteContext"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="promoteContent"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="promoteContent"
            length="2000"
            not-null="false"
            unique="false"
        />
        <property
            name="promoteMonthod"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="promoteMonthod"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="fileId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="fileId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="projectId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="projectId"
            length="19"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
