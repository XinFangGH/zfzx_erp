<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.pawn.pawnItems.VPawnItemsList"
        table="v_pawn_items_list"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="pawnItemId"
            column="pawnItemId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property name="pawnItemType"
            type="java.lang.Integer"
            access="property"
            column="pawnItemType"
            length="10"
        />
        <property
            name="pawnItemName"
            type="java.lang.String"
            access="property"
            column="pawnItemName"
            length="255"
        />
        <property
            name="specificationsStatus"
            type="java.lang.String"
            access="property"
            column="specificationsStatus"
            length="255"
        />
        <property
            name="counts"
            type="java.lang.Integer"
            access="property"
            column="counts"
            length="10"
        />
        <property
            name="assessedValuationValue"
            type="java.lang.String"
            access="property"
            column="assessedValuationValue"
            length="255"
        />
        <property
            name="discountRate"
            type="java.math.BigDecimal"
            access="property"
            column="discountRate"
            precision="20"
            scale="10"
        />
        <property
            name="pawnItemMoney"
            type="java.math.BigDecimal"
            access="property"
            column="pawnItemMoney"
            precision="20"
            scale="2"
        />
        <property
            name="accessTime"
            type="java.util.Date"
            access="property"
            column="accessTime"
            length="19"
        />
        <property
            name="remarks"
            type="java.lang.String"
            access="property"
            column="remarks"
            length="255"
        />
        <property
            name="projectId"
            type="java.lang.Long"
            access="property"
            column="projectId"
            length="19"
        />
        <property
            name="businessType"
            type="java.lang.String"
            access="property"
            column="businessType"
            length="100"
        />
        <property
            name="projectName"
            type="java.lang.String"
            access="property"
            column="projectName"
            length="255"
        />
        <property
            name="projectNumber"
            type="java.lang.String"
            access="property"
            column="projectNumber"
            length="100"
        />
        <property
            name="customerName"
            type="java.lang.String"
            access="property"
            column="customerName"
            length="255"
        />
         <property
            name="pawnItemTypeName"
            type="java.lang.String"
            access="property"
            column="pawnItemTypeName"
            length="100"
        />
         <property
            name="pawnItemStatus"
            type="java.lang.String"
            access="property"
            column="pawnItemStatus"
            length="100"
        />
         <property
            name="startDate"
            type="java.util.Date"
            access="property"
            column="startDate"
            length="19"
        />
         <property
            name="intentDate"
            type="java.util.Date"
            access="property"
            column="intentDate"
            length="19"
        />
     </class>
</hibernate-mapping>
