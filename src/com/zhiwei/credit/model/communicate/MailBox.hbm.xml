<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping> 
  <class name="com.zhiwei.credit.model.communicate.MailBox" table="mail_box" select-before-update="false" dynamic-update="false" dynamic-insert="false" optimistic-lock="version"> 
    <id name="boxId" column="boxId" type="java.lang.Long" length="19"> 
      <generator class="native"></generator> 
    </id>  
    <property name="sendTime" type="java.util.Date" update="true" insert="true" access="property" column="sendTime" length="19" not-null="true" unique="false"/>  
    <property name="delFlag" type="java.lang.Short" update="true" insert="true" access="property" column="delFlag" length="5" not-null="true" unique="false"/>  
    <property name="readFlag" type="java.lang.Short" update="true" insert="true" access="property" column="readFlag" length="5" not-null="true" unique="false"/>  
    <property name="note" type="java.lang.String" update="true" insert="true" access="property" column="note" length="256" not-null="false" unique="false"/>  
    <property name="replyFlag" type="java.lang.Short" update="true" insert="true" access="property" column="replyFlag" length="5" not-null="true" unique="false"/>  
    <!-- imported bi-directional many-to-one association to Mail -->  
    <many-to-one name="mail" class="com.zhiwei.credit.model.communicate.Mail" not-null="false" fetch="select"> 
      <column name="mailId"/> 
    </many-to-one>  
    <!-- imported bi-directional many-to-one association to AppUser -->  
    <many-to-one name="appUser" class="com.zhiwei.credit.model.system.AppUser" not-null="false" fetch="select"> 
      <column name="userId"/> 
    </many-to-one>  
    <!-- imported bi-directional many-to-one association to MailFolder -->  
    <many-to-one name="mailFolder" class="com.zhiwei.credit.model.communicate.MailFolder" not-null="false" fetch="select"> 
      <column name="folderId"/> 
    </many-to-one> 
  </class> 
</hibernate-mapping>
