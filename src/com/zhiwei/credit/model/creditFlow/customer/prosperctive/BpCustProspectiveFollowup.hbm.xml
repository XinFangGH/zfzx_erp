<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.customer.prosperctive.BpCustProspectiveFollowup"
        table="bp_cust_prospective_followup"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="followId"
            column="followId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="followPersonId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="followPersonId"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="followDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="followDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="followType"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="followType"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="followTitle"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="followTitle"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="followInfo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="followInfo"
            length="65535"
            not-null="false"
            unique="false"
        />
        <property
            name="successRate"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="successRate"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="followUpStatus"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="followUpStatus"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="commentorrId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="commentorrId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="commentRemark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="commentRemark"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="customerSystematics"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="customerSystematics"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="personType"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="personType"
            length="5"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to BpCustProsperctive -->
		<many-to-one
			name="bpCustProsperctive"
			class="com.zhiwei.credit.model.creditFlow.customer.prosperctive.BpCustProsperctive"
            not-null="false"
			fetch="select"
		>	
			<column name="perId" />
		</many-to-one>
    </class>
</hibernate-mapping>
