<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="compensatory" extends="struts-default" namespace="/compensatory">
		<action name="*PlBidCompensatory" class="PlBidCompensatoryAction" method="{1}">
			<result>${successResultValue}</result>
			<result name="display">/pages/indexpages/OverDuePlanListPage.jsp</result>
		</action>
	
		<action name="*PlBidCompensatoryFlow" class="PlBidCompensatoryFlowAction" method="{1}">
			<result>${successResultValue}</result>
			
		</action>
	</package>
	
</struts>
