<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.p2p.OaHolidayMessage"
        table="oa_holiday_message"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="10"
        >
            <generator class="native"/>
        </id>
         <property
            name="holidayName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="holidayName"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="message"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="message"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="state"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="state"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="holidayDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="holidayDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="type"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="type"
            length="10"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
