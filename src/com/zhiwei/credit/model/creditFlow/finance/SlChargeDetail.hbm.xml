<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.finance.SlChargeDetail"
        table="sl_charge_detail"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="chargeDetailId"
            column="chargeDetailId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
          <many-to-one
			name="slActualToCharge"
			class="com.zhiwei.credit.model.creditFlow.finance.SlActualToCharge"
			
		>	
			<column name="actualChargeId" />
		</many-to-one>
        	<many-to-one
			name="slFundQlide"
			class="com.zhiwei.credit.model.creditFlow.finance.SlFundQlide"
		>	
			<column name="fundQlideId" />
		</many-to-one>
        <property
            name="overdueNum"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="overdueNum"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="overdueAccrual"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="overdueAccrual"
            precision="10"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="operTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="operTime"
            length="19"
            not-null="false"
            unique="false"
        />
         <property
            name="factDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="factDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="afterMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="afterMoney"
            precision="10"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="transactionType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="transactionType"
            length="250"
            not-null="false"
            unique="false"
        />
            <property
            name="checkuser"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="checkuser"
            length="50"
            not-null="false"
            unique="false"
        />
         <property
            name="iscancel"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="iscancel"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="cancelremark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="cancelremark"
            length="255"
            not-null="false"
            unique="false"
        />
         <property
            name="companyId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="companyId"
            length="19"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
