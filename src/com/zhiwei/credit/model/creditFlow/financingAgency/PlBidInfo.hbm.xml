<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.financingAgency.PlBidInfo"
        table="pl_bid_info"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="userName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="userName"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="userId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="userId"
            length="19"
            not-null="false"
            unique="false"
        />
         <property
            name="couponId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="couponId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="bidName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bidName"
            length="150"
            not-null="false"
            unique="false"
        />
        <property
            name="userMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="userMoney"
            precision="10"
            scale="3"
            not-null="false"
            unique="false"
        />
        <property
            name="bidtime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="bidtime"
            length="19"
            not-null="false"
            unique="false"
        />
         <property
            name="orderNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="orderNo"
            length="100"
            not-null="false"
            unique="false"
        />
          <property
            name="preAuthorizationNum"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="preAuthorizationNum"
            length="255"
            not-null="false"
            unique="false"
        />
         <property
            name="freezeTrxId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="freezeTrxId"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="trxId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="trxId"
            length="255"
            not-null="false"
            unique="false"
        />
         <property
            name="state"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="state"
            length="5"
            not-null="false"
            unique="false"
        />
         <property
            name="newInvestPersonId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="newInvestPersonId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="newInvestPersonName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="newInvestPersonName"
            length="255"
            not-null="false"
            unique="false"
        />
         <property
            name="newOrderNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="newOrderNo"
            length="255"
            not-null="false"
            unique="false"
        />
         <property
            name="isToObligatoryRightChildren"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isToObligatoryRightChildren"
            length="5"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to PlBidPlan -->
		<many-to-one
			name="plBidPlan"
			class="com.zhiwei.credit.model.creditFlow.financingAgency.PlBidPlan"
            not-null="false"
			fetch="select"
		>	
			<column name="bidId" />
		</many-to-one>
    </class>
</hibernate-mapping>
