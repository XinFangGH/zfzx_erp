<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.pawn.project.PawnTicketLossRecord"
        table="pawn_ticket_loss_record"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="lossRecordId"
            column="lossRecordId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="lossTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="lossTime"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="lossPerson"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="lossPerson"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="lossCost"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="lossCost"
            precision="20"
            scale="10"
            not-null="false"
            unique="false"
        />
        <property
            name="operatorId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="operatorId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="operatorName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="operatorName"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="remarks"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remarks"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createTime"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="projectId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="projectId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="businessType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="businessType"
            length="100"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
