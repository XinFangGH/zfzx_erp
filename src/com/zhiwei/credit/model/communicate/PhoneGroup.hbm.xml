<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping> 
  <class name="com.zhiwei.credit.model.communicate.PhoneGroup" table="phone_group" select-before-update="false" dynamic-update="false" dynamic-insert="false" optimistic-lock="version"> 
    <id name="groupId" column="groupId" type="java.lang.Long" length="19"> 
      <generator class="native"></generator> 
    </id>  
    <property name="groupName" type="java.lang.String" update="true" insert="true" access="property" column="groupName" length="128" not-null="true" unique="false"/>  
    <property name="isShared" type="java.lang.Short" update="true" insert="true" access="property" column="isShared" length="5" not-null="true" unique="false"/>  
    <property name="sn" type="java.lang.Integer" update="true" insert="true" access="property" column="SN" length="10" not-null="true" unique="false"/>  
    <property name="isPublic" type="java.lang.Short" update="true" insert="true" access="property" column="isPublic" length="5" not-null="true" unique="false"/>  
    <!-- imported bi-directional many-to-one association to AppUser -->  
    <many-to-one name="appUser" class="com.zhiwei.credit.model.system.AppUser" not-null="false" fetch="select"> 
      <column name="userId"/> 
    </many-to-one>  
    <!-- exported bi-directional one-to-many association to PhoneBook -->  
    <set name="phoneBooks" table="phone_book" lazy="true" inverse="true" cascade="save-update,delete-orphan" sort="unsorted"> 
      <key> 
        <column name="groupId"/> 
      </key>  
      <one-to-many class="com.zhiwei.credit.model.communicate.PhoneBook"/> 
    </set> 
  </class> 
</hibernate-mapping>
