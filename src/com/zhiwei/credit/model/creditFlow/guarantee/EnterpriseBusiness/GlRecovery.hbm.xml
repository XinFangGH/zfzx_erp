<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.guarantee.EnterpriseBusiness.GlRecovery"
        table="gl_recovery"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="recoveryId"
            column="recoveryId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="projectId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="projectId"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="businessType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="businessType"
            length="50"
            not-null="true"
            unique="false"
        />
        <property
            name="compensatoryAmount"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="compensatoryAmount"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="compensatoryBalance"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="compensatoryBalance"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="compensatoryDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="compensatoryDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="recoveryMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="recoveryMoney"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="recoveryDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="recoveryDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="recoverySource"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="recoverySource"
            length="32"
            not-null="false"
            unique="false"
        />
        <property
            name="recoveryRemarks"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="recoveryRemarks"
            length="100"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
