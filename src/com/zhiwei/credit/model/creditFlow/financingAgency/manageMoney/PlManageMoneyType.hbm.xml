<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.financingAgency.manageMoney.PlManageMoneyType"
        table="pl_manageMoney_type"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="manageMoneyTypeId"
            column="manageMoneyTypeId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="name"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="keystr"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="keystr"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remark"
            length="100"
            not-null="false"
            unique="false"
        />
         <property
            name="state"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="state"
            length="10"
            not-null="false"
            unique="false"
        />
        <property 
        	name="typeKey"
        	type="java.lang.String"
        	update="true"
        	insert="true"
        	access="property"
        	column="typeKey"
        	length="20"
        	not-null="false"
        	unique="false"
        />
        <property
            name="nextPublisPlanTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="nextPublisPlanTime"
            length="10"
            not-null="false"
            unique="false"
        />
         <property
            name="receivablesAccount"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="receivablesAccount"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="accountType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="accountType"
            length="255"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
