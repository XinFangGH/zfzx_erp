<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.finance.SlPunishDetail"
        table="sl_punish_detail"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="punishDetailId"
            column="punishDetailId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="fundQlideId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="fundQlideId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="punishInterestId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="punishInterestId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="operTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="operTime"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="afterMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="afterMoney"
            precision="10"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="factDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="factDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="transactionType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="transactionType"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="checkuser"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="checkuser"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="iscancel"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="iscancel"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="cancelremark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="cancelremark"
            length="255"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
