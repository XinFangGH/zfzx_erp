<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.financingAgency.manageMoney.PlEarlyRedemption"
        table="pl_early_redemption"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="earlyRedemptionId"
            column="earlyRedemptionId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
        <property
            name="creator"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="creator"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="earlyDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="earlyDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="earlyMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="earlyMoney"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="residualPrincipal"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="residualPrincipal"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="penaltyDays"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="penaltyDays"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="liquidatedDamagesRate"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="liquidatedDamagesRate"
            precision="20"
            scale="10"
            not-null="false"
            unique="false"
        />
        <property
            name="liquidatedDamages"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="liquidatedDamages"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
         <property
            name="checkStatus"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="checkStatus"
            length="6"
            not-null="false"
            unique="false"
        />
        <property
            name="keystr"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="keystr"
            length="50"
            not-null="false"
            unique="false"
        />
         <property
            name="loanerRepayMentStatus"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="loanerRepayMentStatus"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="loanerRequestNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="loanerRequestNo"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="investRequestNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="investRequestNo"
            length="50"
            not-null="false"
            unique="false"
        />
         <property
            name="realTranseactionAmount"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="realTranseactionAmount"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <many-to-one
			name="plManageMoneyPlanBuyinfo"
			class="com.zhiwei.credit.model.creditFlow.financingAgency.manageMoney.PlManageMoneyPlanBuyinfo"
            not-null="false"
			fetch="select"
		>	
			<column name="orderId" />
		</many-to-one>
     </class>
</hibernate-mapping>
