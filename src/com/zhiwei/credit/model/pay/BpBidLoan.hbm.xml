<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.pay.BpBidLoan"
        table="bp_bid_loan"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="bidLoanId"
            column="bidLoanId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="platFormId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="pId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="loanNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="loanNo"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="orderNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="orderNo"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="batchNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="batchNo"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="amount"
            type="java.lang.Double"
            update="true"
            insert="true"
            access="property"
            column="amount"
            length="22"
            not-null="false"
            unique="false"
        />
        <property
            name="transferName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="transferName"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remark"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="loanOutFlag"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="loanOutFlag"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="loanInFlag"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="loanInFlag"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="state"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="state"
            length="5"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
