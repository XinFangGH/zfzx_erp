<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.guarantee.guaranteefinance.GlAccountBank"
        table="gl_account_bank"
        select-before-update="false"
        dynamic-update="true"
        dynamic-insert="true"
        optimistic-lock="all"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
         <generator class="assigned"></generator>
        </id>
         <property
            name="bankParentId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="bankParentId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="authorizationMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="authorizationMoney"
             precision="20"
            scale="6"
            not-null="false"
            unique="false"
        />
         <property
            name="surplusMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="surplusMoney"
             precision="20"
            scale="6"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remark"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="leaf"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="leaf"
            length="4"
            not-null="false"
            unique="false"
        />
        <property
            name="text"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="text"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="serviceTypeBank"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="serviceTypeBank"
            length="100"
            not-null="false"
            unique="false"
        />
         <property
           name="idDelete"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="idDelete"
            length="5"
            not-null="false"
            unique="false"
        />
         <property
            name="companyId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="companyId"
            length="19"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
