<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.system.SysDatabaseBR"
        table="sys_databaseBR"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="fileName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="fileName"
            length="250"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="createDate"
            length="250"
            not-null="false"
            unique="false"
        />
        <property
            name="isAutoCreate"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="isAutoCreate"
            length="10"
            not-null="false"
            unique="false"
        />
         <property
            name="fileSize"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="fileSize"
            length="11"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
