<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.finance.SlLoanAccountErrorLog"
        table="sl_loan_account_error_log"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="accountErrorId"
            column="accountErrorId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="corpno"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="corpno"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="duebillno"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="duebillno"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="custname"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="custname"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="custtype"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="custtype"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="loantype"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="loantype"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="custbankname"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="custbankname"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="bankname"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bankname"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="accountno"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="accountno"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="accounttype"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="accounttype"
            length="30"
            not-null="false"
            unique="false"
        />
         <property
            name="companyId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="companyId"
            length="19"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
