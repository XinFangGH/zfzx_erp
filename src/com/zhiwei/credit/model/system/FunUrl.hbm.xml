<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping> 
  <class name="com.zhiwei.credit.model.system.FunUrl" table="fun_url" select-before-update="false" dynamic-update="false" dynamic-insert="false" optimistic-lock="version"> 
    <id name="urlId" column="urlId" type="java.lang.Long" length="19"> 
      <generator class="native"></generator> 
    </id>  
    <property name="urlPath" type="java.lang.String" update="true" insert="true" access="property" column="urlPath" length="128" not-null="true" unique="false"/>  
    <!-- imported bi-directional many-to-one association to AppFunction -->  
    <many-to-one name="appFunction" class="com.zhiwei.credit.model.system.AppFunction" not-null="false" fetch="select"> 
      <column name="functionId"/> 
    </many-to-one> 
  </class> 
</hibernate-mapping>
