<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.archives.PlArchives"
        table="pl_archives"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="20"
        >
            <generator class="native"/>
        </id>
         <property
            name="code"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="code"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="name"
            length="110"
            not-null="false"
            unique="false"
        />
        <property
            name="sortorder"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="sortorder"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="parentid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="parentid"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="remarks"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remarks"
            length="500"
            not-null="false"
            unique="false"
        />
                <property
            name="leaf"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="leaf"
            length="4"
            not-null="false"
            unique="false"
        />
        <property
            name="text"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="text"
            length="255"
            not-null="false"
            unique="false"
        />
            <property
           name="isdelete"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isdelete"
            length="5"
            not-null="false"
            unique="false"
        />
            <property
            name="companyId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="companyId"
            length="19"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
