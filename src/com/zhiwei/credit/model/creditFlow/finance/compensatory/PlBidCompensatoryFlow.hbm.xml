<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.finance.compensatory.PlBidCompensatoryFlow"
        table="pl_bid_compensatory_flow"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="compensatoryId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="compensatoryId"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="backPunishMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="backPunishMoney"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="backCompensatoryMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="backCompensatoryMoney"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="flateMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="flateMoney"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="backType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="backType"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="requestNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="requestNo"
            length="255"
            not-null="false"
            unique="true"
        />
        <property
            name="backStatus"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="backStatus"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="backDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="backDate"
            length="19"
            not-null="false"
            unique="false"
        />
        
     </class>
</hibernate-mapping>
