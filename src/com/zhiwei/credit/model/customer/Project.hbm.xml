<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping> 
  <class name="com.zhiwei.credit.model.customer.Project" table="project" select-before-update="false" dynamic-update="false" dynamic-insert="false" optimistic-lock="version"> 
    <id name="projectId" column="projectId" type="java.lang.Long" length="19"> 
      <generator class="native"></generator> 
    </id>  
    <property name="projectName" type="java.lang.String" update="true" insert="true" access="property" column="projectName" length="128" not-null="true" unique="false"/>  
    <property name="projectNo" type="java.lang.String" update="true" insert="true" access="property" column="projectNo" length="64" not-null="true" unique="false"/>  
    <property name="reqDesc" type="java.lang.String" update="true" insert="true" access="property" column="reqDesc" length="65535" not-null="false" unique="false"/>  
    <property name="isContract" type="java.lang.Short" update="true" insert="true" access="property" column="isContract" length="5" not-null="true" unique="false"/>  
    <property name="fullname" type="java.lang.String" update="true" insert="true" access="property" column="fullname" length="32" not-null="true" unique="false"/>  
    <property name="mobile" type="java.lang.String" update="true" insert="true" access="property" column="mobile" length="32" not-null="false" unique="false"/>  
    <property name="phone" type="java.lang.String" update="true" insert="true" access="property" column="phone" length="32" not-null="false" unique="false"/>  
    <property name="fax" type="java.lang.String" update="true" insert="true" access="property" column="fax" length="32" not-null="false" unique="false"/>  
    <property name="otherContacts" type="java.lang.String" update="true" insert="true" access="property" column="otherContacts" length="128" not-null="false" unique="false"/>  
    <!-- imported bi-directional many-to-one association to AppUser -->  
    <many-to-one name="appUser" class="com.zhiwei.credit.model.system.AppUser" not-null="false" fetch="select"> 
      <column name="userId"/> 
    </many-to-one>  
    <!-- imported bi-directional many-to-one association to Customer -->  
    <many-to-one name="customer" class="com.zhiwei.credit.model.customer.Customer" not-null="false" fetch="select"> 
      <column name="customerId"/> 
    </many-to-one>  
    <!-- exported bi-directional one-to-many association to Contract -->  
    <set name="contracts" table="contract" lazy="true" inverse="true" cascade="save-update,delete-orphan" sort="unsorted"> 
      <key> 
        <column name="projectId"/> 
      </key>  
      <one-to-many class="com.zhiwei.credit.model.customer.Contract"/> 
    </set>  
    <!-- exported bi-directional one-to-many association to ProjectFile -->  
    <set name="projectFiles" table="project_file" lazy="true" cascade="save-update,delete-orphan"> 
      <key> 
        <column name="projectId"/> 
      </key>  
      <many-to-many class="com.zhiwei.credit.model.system.FileAttach" column="fileId" fetch="join"/> 
    </set> 
  </class> 
</hibernate-mapping>
