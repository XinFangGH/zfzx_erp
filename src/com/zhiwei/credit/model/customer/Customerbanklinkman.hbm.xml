<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.customer.Customerbanklinkman"
        table="customerbanklinkman"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="name"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="pos"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="pos"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="sex"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="sex"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="phone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="phone"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="main"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="main"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="address"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="address"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="enterpriseid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="enterpriseid"
            length="19"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
