<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.materials.GlobalGuaranteeMaterialsExt"
        table="global_guarantee_materials_ext"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="materialsId"
            column="materialsId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="materialsName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="materialsName"
            length="255"
            not-null="false"
            unique="false"
        />
         <property
            name="remarks"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remarks"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="operationTypeKey"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="operationTypeKey"
            length="100"
            not-null="false"
            unique="false"
        />
        
        <property
            name="ruleExplain"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="ruleExplain"
            length="1000"
            not-null="false"
            unique="false"
        />
        <property
            name="parentId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="parentId"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="leaf"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="leaf"
            length="10"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
