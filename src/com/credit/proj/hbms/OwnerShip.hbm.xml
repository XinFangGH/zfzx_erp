<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
       <class
        name="com.credit.proj.settlecenter.entity.OwnerShip"
        table="owner_ship"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
     >
    
		<id
            name="ownershipId"
            column="ownershipId"
            type="java.lang.Long"
            length="20"
        >
            <generator class="native"/>
        </id>
         <property
            name="deptId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="deptId"
            length="20"
            unique="false"
        />
         <property
            name="bidId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="bidId"
            length="20"
            unique="false"
        />
         <property
            name="investId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="investId"
            length="20"
            unique="false"
        />
         <property
            name="infoId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="infoId"
            length="11"
            unique="false"
        />
         <property
            name="borrowerId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="borrowerId"
            length="20"
            unique="false"
        />
        <property
            name="settlementType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="settlementType"
            length="5"
            not-null="false"
            unique="false"
        />
         <property
            name="isSettled"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isSettled"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createDate"
            length="19"
            unique="false"
        />
        <property
            name="transferDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="transferDate"
            length="19"
            unique="false"
        />
        <property
            name="commissionRate"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="commissionRate"
            precision="20"
            scale="10"
            not-null="false"
            unique="false"
        />
         <property
            name="reMainMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="reMainMoney"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="commissionMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="commissionMoney"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="investName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="investName"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="borrower"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="borrower"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="bidName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bidName"
            length="50"
            not-null="false"
            unique="false"
        />
</class>
</hibernate-mapping>
