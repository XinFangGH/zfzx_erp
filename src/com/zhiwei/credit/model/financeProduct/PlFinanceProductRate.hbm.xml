<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.financeProduct.PlFinanceProductRate"
        table="pl_finance_product_rate"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="intentDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="intentDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="yearRate"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="yearRate"
            precision="20"
            scale="6"
            not-null="false"
            unique="false"
        />
        <property
            name="sevYearRate"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="sevYearRate"
            precision="20"
            scale="6"
            not-null="false"
            unique="false"
        />
        <property
            name="dayRate"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="dayRate"
            precision="20"
            scale="6"
            not-null="false"
            unique="false"
        />
        <property
            name="sevDayRate"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="sevDayRate"
            precision="20"
            scale="6"
            not-null="false"
            unique="false"
        />
        <property
            name="productId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="productId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="modifyDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="modifyDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="createUserId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="createUserId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="createUserName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="createUserName"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="companyId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="companyId"
            length="19"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
