<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.p2p.loan.P2pLoanBasisMaterial"
        table="p2p_loan_basisMaterial"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="materialId"
            column="materialId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="materialName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="materialName"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="materialState"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="materialState"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="operationType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="operationType"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remark"
            length="255"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
