<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.finance.SlBankAccount"
        table="sl_bank_account"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="bankAccountId"
            column="bankAccountId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="openType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="openType"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="accountType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="accountType"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="name"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="account"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="account"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="rawMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="rawMoney"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="finalMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="finalMoney"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="recordTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="recordTime"
            length="19"
            not-null="false"
            unique="false"
        />
           <property
            name="finalDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="finalDate"
            length="19"
            not-null="false"
            unique="false"
        />
         <property
            name="remarks"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remarks"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="address"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="address"
            length="255"
            not-null="false"
            unique="false"
        />
         <property
            name="companyId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="companyId"
            length="19"
            not-null="false"
            unique="false"
        />
         <property
            name="bankOutletsName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bankOutletsName"
            length="255"
            not-null="false"
            unique="false"
        />
         <property
            name="openBankId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="openBankId"
            length="19"
            not-null="false"
            unique="false"
        />
    
     </class>
</hibernate-mapping>
