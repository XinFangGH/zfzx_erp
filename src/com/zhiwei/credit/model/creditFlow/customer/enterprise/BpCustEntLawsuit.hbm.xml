<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.customer.enterprise.BpCustEntLawsuit"
        table="bp_cust_ent_lawsuit"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="lawsuitId"
            column="lawsuitId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
    
         <property
            name="registerDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="registerDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="registerReason"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="registerReason"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="relatedMoney"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="relatedMoney"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="role"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="role"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="recordUser"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="recordUser"
            length="255"
            not-null="false"
            unique="false"
        />
     
 <property
            name="recordTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="recordTime"
            length="19"
            not-null="false"
            unique="false"
        />
         <many-to-one name="enterprise" class="com.zhiwei.credit.model.creditFlow.customer.enterprise.Enterprise"   > 
        <column name="enterpriseId"/> 
        </many-to-one> 
     </class>
</hibernate-mapping>
