<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.log.BatchRunRecord"
        table="batch_runRecord"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="runType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="runType"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="appUserId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="appUserId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="appUserName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="appUserName"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="startRunDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="startRunDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="endRunDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="endRunDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="happenAbnorma"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="happenAbnorma"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="totalNumber"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="totalNumber"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="ids"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="ids"
            length="255"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
