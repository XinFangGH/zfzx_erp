<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.system.UploadLog"
        table="upload_log"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="logId"
            column="logId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="bidId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bidId"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="bidName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bidName"
            length="250"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="msg"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="msg"
            length="1025"
            not-null="false"
            unique="false"
        />
        <property
            name="totalCount"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="totalCount"
            length="11"
            not-null="false"
            unique="false"
        />
        <property
            name="successCount"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="successCount"
            length="11"
            not-null="false"
            unique="false"
        />
        <property
            name="failureCount"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="failureCount"
            length="11"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
