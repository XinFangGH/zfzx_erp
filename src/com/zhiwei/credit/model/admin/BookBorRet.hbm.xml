<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping> 
  <class name="com.zhiwei.credit.model.admin.BookBorRet" table="book_bor_ret" select-before-update="false" dynamic-update="false" dynamic-insert="false" optimistic-lock="version"> 
    <id name="recordId" column="recordId" type="java.lang.Long" length="19"> 
      <generator class="native"></generator> 
    </id>  
    <property name="borrowTime" type="java.util.Date" update="true" insert="true" access="property" column="borrowTime" length="19" not-null="true" unique="false"/>  
    <property name="returnTime" type="java.util.Date" update="true" insert="true" access="property" column="returnTime" length="19" not-null="true" unique="false"/>  
    <property name="lastReturnTime" type="java.util.Date" update="true" insert="true" access="property" column="lastReturnTime" length="19" not-null="false" unique="false"/>  
    <property name="borrowIsbn" type="java.lang.String" update="true" insert="true" access="property" column="borrowIsbn" length="128" not-null="true" unique="false"/>  
    <property name="bookName" type="java.lang.String" update="true" insert="true" access="property" column="bookName" length="128" not-null="true" unique="false"/>  
    <property name="registerName" type="java.lang.String" update="true" insert="true" access="property" column="registerName" length="32" not-null="true" unique="false"/>  
    <property name="fullname" type="java.lang.String" update="true" insert="true" access="property" column="fullname" length="32" not-null="true" unique="false"/>  
    <!-- imported bi-directional many-to-one association to BookSn -->  
    <many-to-one name="bookSn" class="com.zhiwei.credit.model.admin.BookSn" not-null="false" fetch="select"> 
      <column name="bookSnId"/> 
    </many-to-one> 
  </class> 
</hibernate-mapping>
