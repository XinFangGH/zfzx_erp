<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.contract.CsElementCategory"
        table="cs_element_category"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="elementCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="elementCode"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="description"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="method"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="method"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="businessType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="businessType"
            length="100"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
