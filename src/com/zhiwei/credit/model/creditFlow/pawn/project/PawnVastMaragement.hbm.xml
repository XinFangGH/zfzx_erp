<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.pawn.project.PawnVastMaragement"
        table="pawn_vast_maragement"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="vastId"
            column="vastId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="vastNumber"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="vastNumber"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="vastWay"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="vastWay"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="managerId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="managerId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="managerName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="managerName"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="vastDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="vastDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="remarks"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remarks"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="projectId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="projectId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="businessType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="businessType"
            length="100"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
