<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.ourmain.SlPersonMain"
        table="sl_person_main"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="personMainId"
            column="personMainId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="name"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="sex"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="sex"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="cardtype"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="cardtype"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="cardnum"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="cardnum"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="linktel"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="linktel"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="address"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="address"
            length="250"
            not-null="false"
            unique="false"
        />
        <property
            name="tel"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="tel"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="home"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="home"
            length="250"
            not-null="false"
            unique="false"
        />
        <property
            name="postalCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="postalCode"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="tax"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="tax"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="isPledge"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isPledge"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="companyId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="companyId"
            length="20"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
