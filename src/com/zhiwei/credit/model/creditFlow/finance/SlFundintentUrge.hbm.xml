<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.finance.SlFundintentUrge"
        table="sl_fundintent_urge"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="slFundintentUrgeId"
            column="slFundintentUrgeId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="urgeType"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="urgeType"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="urgeTitle"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="urgeTitle"
            length="250"
            not-null="false"
            unique="false"
        />
        <property
            name="urgeContext"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="urgeContext"
            length="2000"
            not-null="false"
            unique="false"
        />
        <property
            name="urgeTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="urgeTime"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="urgePerson"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="urgePerson"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="fundIntentId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="fundIntentId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="projectId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="projectId"
            length="19"
            not-null="true"
            unique="false"
        />
     </class>
</hibernate-mapping>
