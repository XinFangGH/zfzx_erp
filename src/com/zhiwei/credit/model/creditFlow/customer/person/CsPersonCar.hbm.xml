<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.creditFlow.customer.person.CsPersonCar"
        table="cs_person_car"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id><!--
         <property
            name="personId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="personId"
            length="19"
            not-null="false"
            unique="false"
        />
        --><property
            name="carSystemType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="carSystemType"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="carType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="carType"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="isMortgage"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="isMortgage"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="newCarValue"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="newCarValue"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="loanMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="loanMoney"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="carFactValue"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="carFactValue"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="yearOfCarUse"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="yearOfCarUse"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="finalCertificationPrice"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="finalCertificationPrice"
            precision="20"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="propertyOwner"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="propertyOwner"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="carLicenseNumber"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="carLicenseNumber"
            length="20"
            not-null="false"
            unique="false"
        />
        <many-to-one name="person" 
        		     class="com.zhiwei.credit.model.creditFlow.customer.person.Person"
        		     not-null="false"
        		     fetch="select"
        		     >
        	<column name="personId"></column>
        </many-to-one>
     </class>
</hibernate-mapping>
